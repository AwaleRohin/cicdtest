{
    "executionRoleArn": "arn:aws:iam::031342435657:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "gwell-codedeploy",
            "image": "031342435657.dkr.ecr.ap-south-1.amazonaws.com/cicdtest:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "containerPort": 3000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE",
        "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}